{
  "name": "TG Rename Bot",
  "description": "Telegram's best Open Source Rename Bot. Create your Fork now.",
  "keywords": [
    "telegram",
    "best",
    "open",
    "source",
    "Rename"
  ],
  "success_url": "https://github.com/spechide/AnyDLbot",
  "repository": "https://github.com/amegh-amz/RenameBot-PermTB.git",
  "env": {
    "WEBHOOK": {
      "description": "Setting this to ANYTHING will enable webhooks when in env mode",
      "value": "ANYTHING"
    },
    "TG_BOT_TOKEN": {
      "description": "Your bot token, as a string.",
      "value": ""
    },
    "APP_ID": {
      "description": "Get this value from https://my.telegram.org",
      "value": "2025851"
    },
    "API_HASH": {
      "description": "Get this value from https://my.telegram.org",
      "value": "7d11f5c498b255906d40877e8a757062"
    },
    "AUTH_USERS": {
      "description": "allow only pre-defined users to use this bot",
      "value": "1167010511 1666422511"
    },
    "BANNED_USERS": {
      "description": "Banned Unwanted members..",
      "value": "",
      "required": false
    }
  },
  "addons": [
    {
    "plan": "heroku-postgresql",
      "options": {
        "version": "12"
      }
    }
  ],
  "buildpacks": [{
    "url": "heroku/python"
  }],
  "formation": {
    "worker": {
      "quantity": 1,
      "size": "free"
    }
  }
}
